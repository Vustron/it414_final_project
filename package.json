{
  "name": "it414_final_project",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome lint --write --unsafe src",
    "format": "bunx @biomejs/biome format --write src",
    "fmt:fix": "bun run lint && bun run format && bun run typecheck",
    "check-dep": "bunx upgradeps -v",
    "prod-test": "turbo build && turbo start",
    "prepare": "husky",
    "generate": "bunx prisma generate --schema ./src/prisma/schema.prisma",
    "push": "bunx prisma db push --schema ./src/prisma/schema.prisma",
    "studio": "bunx prisma studio --schema ./src/prisma/schema.prisma",
    "migrate": "bunx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "postinstall": "prisma generate"
  },
